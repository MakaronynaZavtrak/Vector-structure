#pragma once
#include <string>
#include <iostream>
#include <math.h>

using namespace std;
struct Vector {

	double x;
	double y;
	double z;

	Vector(double x1, double y1, double z1) {
		this->x = x1;
		this->y = y1;
		this->z = z1;
	}

	Vector() {
		this->x = 0;
		this->y = 0;
		this->z = 0;
	}
};

double mixedMultiplication(Vector a, Vector b, Vector c);
void printNewVector(Vector a);
template <typename T>
void printf(T result);
template <class S>
void printfEndl(S result);
void findNormalVector(Vector a, Vector b);
void someTMultiplication(double t, Vector a);
void summVectors(Vector a, Vector b);
void findLength(Vector a);
void shortedNormed(Vector a);
void scalMultiplication(Vector a, Vector b);
Vector vectorMultiplication(Vector a, Vector b);
